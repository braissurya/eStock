<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

   <rule>
        <from>^.*\.(js|css|gif|png|jpg|jpeg)$</from>
        <set type="expires">31536000 second</set>
  </rule>

</urlrewrite>